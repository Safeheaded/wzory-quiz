{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:react/recommended",
        "prettier/@typescript-eslint",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/display-name": "error"
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    }
}